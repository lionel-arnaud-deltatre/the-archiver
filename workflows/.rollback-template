name: Rollback <target>

on:
  workflow_dispatch:
    inputs:
      version:
        description: 'Select version to rollback'
        required: true
        type: choice
        options:
          <versions_placeholder>

jobs:
  rollback:
    runs-on: ubuntu-latest
    steps:
      - name: Rollback to Selected Version
        run: echo "Rolling back to ${{ github.event.inputs.version }}"
